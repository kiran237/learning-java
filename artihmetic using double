class BasicMath
{
public static void main(String args[])
{
//arthmetic using integers
System.out.println("integer arthmetic");
double da=1+1;
double db=da*3;
double dc=db/4;
double dd=dc-a;
double de=-dd;
System.out.println("da="+da);
System.out.println("db="+db);
